<simulation name="Simple RangeEX">
	<!-- each simulation must have:
	generations
	min and max, lat and lon
	carrying capacity
	hardborders
	softborders
	reproductiveability
	dispersalradius
	initialpopulation
	output
	-->

	<generations>1000</generations>
	
	<threads>1</threads>
	
	<latitude minimum="-10.0" maximum="10.0"/>
	<longitude minimum="-10.0" maximum="10.0"/>
	
	<edges type="impenetrable"/>

	<carryingcapacity>
		<data type="file" startgeneration="1" endgeneration="500">center.txt</data>
		<data type="file" startgeneration="501" endgeneration="1000">center2.txt</data>
     </carryingcapacity>

	<hardborders>
		<data type="file">blank.txt</data>
	</hardborders>

	<softborders>
			<data type="file">boxborder.txt</data>
	</softborders>
	
	<reproductiveability>
		<distribution startgeneration="1" endgeneration="500" offspring="2" p="1"/>
		<distribution startgeneration="501" endgeneration="1000" offspring="1 2" p="0.5 0.5"/>
	</reproductiveability>
	
	<dispersalradius>
		<distribution radii="0.5 2.5 5 7.5 10 12.5 15 17.5 20 22.5 25 27.5 30 35 40 45 50" p="0.15 0.3 0.18 0.11 0.07 0.05 0.03 0.02 0.02 0.015 0.01 0.01 0.01 0.01 0.007 0.005 0.003"/>
		<!-- <distribution radii="100" p="1"/> -->
	</dispersalradius>
	
	<initialpopulation>
		<node lat="0" lon="0" n="5" r="255" g="255" b="0"/>
		<node lat="-5" lon="-5" n="5" r="0" g="255" b="0"/>
	</initialpopulation>
	<output>
		<visual/>
	</output>
	
		
</simulation>

